; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "ActorResource",
"language": "GDScript",
"path": "res://tools/grog/actor_resource.gd"
}, {
"base": "Reference",
"class": "CompiledGrogScript",
"language": "GDScript",
"path": "res://tools/grog/compiled_grog_script.gd"
}, {
"base": "Reference",
"class": "EventQueue",
"language": "GDScript",
"path": "res://tools/misc/event_queue.gd"
}, {
"base": "Resource",
"class": "GameResource",
"language": "GDScript",
"path": "res://tools/grog/game_resource.gd"
}, {
"base": "Reference",
"class": "GameServer",
"language": "GDScript",
"path": "res://tools/grog/game_server.gd"
}, {
"base": "Reference",
"class": "GrogCompiler",
"language": "GDScript",
"path": "res://tools/grog/grog_compiler.gd"
}, {
"base": "Resource",
"class": "GrogScript",
"language": "GDScript",
"path": "res://tools/grog/grog_script.gd"
}, {
"base": "Resource",
"class": "RoomResource",
"language": "GDScript",
"path": "res://tools/grog/room_resource.gd"
} ]
_global_script_class_icons={
"ActorResource": "",
"CompiledGrogScript": "",
"EventQueue": "",
"GameResource": "",
"GameServer": "",
"GrogCompiler": "",
"GrogScript": "",
"RoomResource": ""
}

[application]

config/name="Grog"
run/main_scene="res://apps/room_tester/room_tester.tscn"
config/icon="res://icon.png"

[autoload]

grog="*res://tools/grog/grog.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1366
window/size/test_height=768
window/stretch/mode="2d"
window/stretch/aspect="keep"

[locale]

translations=PoolStringArray( "res://apps/room_tester/translations.en.translation", "res://apps/room_tester/translations.es.translation" )

[rendering]

environment/default_environment="res://default_env.tres"
